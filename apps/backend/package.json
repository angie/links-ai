{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "lint": "eslint .",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:smoke": "sst bind --stage dev 'vitest run --dir smoke-tests -c vitest.config.smoke.mjs'",
    "test:smoke:local": "sst bind --stage local 'vitest run --dir smoke-tests -c vitest.config.smoke.mjs'",
    "typecheck": "tsc --noEmit",
    "insomnia": "sst bind scripts/generate-insomnia-env.mjs"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/node": "^20.8.8",
    "aws-cdk-lib": "2.95.1",
    "constructs": "10.2.69",
    "eslint-config-custom": "workspace:^",
    "eslint-import-resolver-alias": "^1.1.2",
    "logger": "workspace:^",
    "sst": "^2.26.5",
    "tsconfig": "workspace:^",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "vitest-config": "workspace:^"
  }
}
